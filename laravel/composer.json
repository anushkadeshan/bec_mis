{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "~4.6.",
        "beyondcode/laravel-websockets": "^1.3",
        "chumper/zipper": "1.0.x",
        "fideloper/proxy": "^4.0",
        "google/cloud-firestore": "^1.19",
        "grpc/grpc": "^v1.27.0",
        "guzzlehttp/guzzle": "~6.0",
        "kim/activity": "^1.1",
        "kreait/firebase-php": "4.14.1",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "matthewbdaly/laravel-azure-storage": "^1.3",
        "morrislaptop/firestore-php": "^2.2",
        "nao-pon/flysystem-google-drive": "~1.1",
        "owen-it/laravel-auditing": "^8.0",
        "pusher/pusher-php-server": "~4.0",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sendgrid/sendgrid": "~7",
        "spatie/db-dumper": "^2.15",
        "spatie/laravel-backup": "6.0.0",
        "tymon/jwt-auth": "dev-develop"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
